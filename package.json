{
	"name": "electronbar",
	"version": "1.2.1",
	"description": "A react title bar component and handler for Electron.",
	"author": "Jeff Seaman",
	"contributors": [
		"Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"prepare": "rollup -c && copyfiles -u 1 css/* lib"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties",
			"@babel/transform-react-jsx"
		],
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"chrome": "60"
					}
				}
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.5.4",
		"@babel/plugin-proposal-class-properties": "^7.5.0",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/preset-env": "^7.5.4",
		"copyfiles": "^2.1.1",
		"rollup": "^1.17.0",
		"rollup-plugin-babel": "^4.3.3"
	},
	"dependencies": {
		"react": "^16.8.6",
		"react-dom": "^16.8.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/obsius/electronbar.git"
	},
	"keywords": [
		"electron",
		"titlebar",
		"menubar",
		"title",
		"menu",
		"frameless",
		"custom"
	]
}
