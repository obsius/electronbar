{
	"name": "electronbar",
	"version": "1.0.10",
	"description": "A react title bar component and handler for Electron.",
	"author": "Jeff Seaman",
	"contributors": [
		"Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"prepare": "babel src -d lib"
	},
	"babel": {
		"plugins": [
			"transform-runtime",
			"transform-decorators-legacy",
			"transform-react-jsx",
			[
				"babel-plugin-transform-builtin-extend",
				{
					"globals": [
						"Error",
						"Array"
					]
				}
			],
			[
				"babel-plugin-root-import",
				{
					"rootPathSuffix": "src/",
					"rootPathPrefix": "~"
				}
			]
		],
		"presets": [
			"env",
			"stage-2"
		]
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-root-import": "^6.1.0",
		"babel-plugin-transform-builtin-extend": "^1.1.2",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"react": "^16.8.2",
		"react-dom": "^16.8.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/obsius/electronbar.git"
	},
	"keywords": [
		"electron",
		"titlebar",
		"menubar",
		"title",
		"menu",
		"frameless",
		"custom"
	]
}
